{
  "name": "@hashicorp/gh-action-check-broken-links",
  "version": "0.0.0",
  "description": "A GitHub Action that checks for the presence of broken links",
  "main": "lib/main.js",
  "scripts": {
    "watch": "tsc --watch",
    "tsc": "tsc",
    "build": "ncc build",
    "test": "vitest",
    "all": "$npm_execpath run tsc && $npm_execpath run build && $npm_execpath run test --run"
  },
  "keywords": [
    "actions",
    "node",
    "link",
    "check"
  ],
  "author": "Kevin Pruett",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@hashicorp/nextjs-scripts": "^19.0.3",
    "rehype-parse": "^9",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.4",
    "unist-util-find": "^3.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@octokit/types": "^13.4.1",
    "@types/glob": "^8.1.0",
    "@types/hast-format": "^2.3.0",
    "@types/mdast": "^4.0.3",
    "@types/node": "^20.12.2",
    "@types/node-fetch": "^2.6.11",
    "@types/unist": "^3.0.2",
    "@vercel/ncc": "^0.38.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
